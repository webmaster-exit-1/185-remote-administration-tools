                        Me.Invoke(j, New Object() {sock, b})
                        Exit Sub
                    End If
                    Dim f As New Process
                    f.sock = sock
                    f.Name = "\\" & sock
                    f.Show()




            End Select
        Catch ex As Exception

        End Try
    End Sub

